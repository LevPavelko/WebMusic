@model WebMusic.Models.GenresAndExecutorsViewModel

<div class="quarter-page">

    <a class="href" asp-area="" asp-controller="Home" asp-action="Index">
        <div class="panel-items">
            <div class="panel-img">
                <img src="/images/house.png" width="35px" height="35px" style="margin-left: 7px; margin-bottom: 15px;margin-top: 10px;" />
                @Resources.Resource.Home
            </div>

        </div>
    </a>

    <a class="href" asp-area="" asp-controller="Home" asp-action="Navigation">
        <div class="panel-items">
            <div class="panel-img">
                <img src="/images/compass.png" width="35px" height="35px" style="margin-left: 7px; margin-bottom: 15px;margin-top: 10px;" />
                @Resources.Resource.Navigation
            </div>

        </div>
    </a>



</div>
<h1 style="color: white">@Resources.Resource.Executors</h1>
<div class="two-columns">
    @foreach (var item in Model.Executors)
    {
        <a class="audio-player" asp-area="" asp-controller="Home" asp-action="GetSongsE" asp-route-id="@item.Id">
            <div>
                <ul>
                   
                    <li style="font-size: 30px;">
                        @Html.DisplayFor(modelItem => item.Name)


                    </li>

                </ul>
            </div>
        </a>
      
    }
</div>

<h1 style="color: white">@Resources.Resource.Genres</h1>
<div class="two-columns">
    @foreach (var item in Model.Genres)
    {
        <a class="audio-player" asp-area="" asp-controller="Home" asp-action="GetSongsG" asp-route-id="@item.Id">
            <div>
                <ul>
                    <li style="font-size: 30px;">
                        @Html.DisplayFor(modelItem => item.Name)


                    </li>

                </ul>
            </div>
        </a>
        
    }
</div>